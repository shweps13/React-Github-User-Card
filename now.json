{
    "version": 2,
    "name": "user-card",
    "builds": [
        {
            "src": "user-card/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "user-card/$1"
        }
    ]
}